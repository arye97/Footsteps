{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 9500",
    "build": "vue-cli-service build --mode staging",
    "build-prod": "vue-cli-service build --mode prod",
    "kill-all": "fuser -k 9500/tcp 9499/tcp",
    "lint": "vue-cli-service lint",
    "unit-test": "jest --testPathIgnorePatterns LiveBackEndApi.test.js --coverage --runInBand --detectOpenHandles",
    "rest-test": "jest --coverage --runInBand --detectOpenHandles LiveBackEndApi.test.js",
    "sonarqube": "node sonarqube-analysis.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap-vue": "^2.16.0",
    "core-js": "^3.6.5",
    "jquery": "^3.5.1",
    "serve": "^11.3.2",
    "vue": "^2.6.11",
    "vue-multiselect": "^2.1.6",
    "vue-router": "^3.4.2",
    "vuejs-logger": "^1.5.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^25.5.4",
    "jest-sonar-reporter": "^2.0.0",
    "sonarqube-scanner": "^2.7.0",
    "vue-jest": "^3.0.6",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testResultsProcessor": "jest-sonar-reporter",
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    },
    "jestSonar": {
      "reportPath": "coverage",
      "reportFile": "test-report.xml",
      "indent": 4
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/assets/css/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node",
      "vue"
    ],
    "setupFilesAfterEnv": [
      "./src/Test/LiveBackEnd/setup.js"
    ],
    "globalSetup": "<rootDir>/jest-setup-global.js"
  }
}
